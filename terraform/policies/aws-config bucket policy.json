            "Principal": {
                "Service": "config.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::aws-config-security-292725948066/AWSLogs/292725948066/Config/*",
            "Condition": {
                "StringEquals": {
                    "AWS:SourceAccount": "292725948066",
                    "s3:x-amz-acl": "bucket-owner-full-control"
                }
            }
